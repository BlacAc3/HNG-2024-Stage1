{
    "builds": [{
        "src": "my_api/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "15mb",
            "installCommand": "pip install -r requirements.txt"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "my_api/wsgi.py"
        }
    ]
}